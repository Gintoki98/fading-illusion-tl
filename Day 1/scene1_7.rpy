translate spanish destitute1_scene1_7_d4af34e9:

    # rei "Судя по тому, что Генсокё не взлетел на воздух, всё прошло успешно?"
    rei "A juzgar por el hecho de que Gensokyo no explotó, ¿fue exitoso?"

# game/script/destitute_chapter/day1/scene1_7.rpy:558
translate spanish destitute1_scene1_7_bd9f2b56:

    # rei "А, точно."
    rei "Oh, cierto..."

# game/script/destitute_chapter/day1/scene1_7.rpy:564
translate spanish destitute1_scene1_7_e43fa46e:

    # rei "А, точно.{fast} Хотя..."
    rei "Oh, cierto.{fast} Aunque..."


