    # game/script/destitute_chapter/day1/scene2_11.rpy:38
    old "Храм Хакурей"
    new "Hakurei Shrine"

    # game/script/destitute_chapter/day1/scene2_11.rpy:40
    old "Ночь"
    new "Night"

